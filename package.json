{
    "name": "@savks/js-container",
    "version": "3.0.0-beta.3",
    "description": "JS Application Container",
    "license": "MIT",
    "author": "Savks",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "prepack": "yarn build"
    },
    "devDependencies": {
        "@tarik02/eslint-config-type-1": "^0.2.1",
        "@tarik02/eslint-config-type-1-import": "^0.2.1",
        "eslint": "^8.37.0",
        "typescript": "^5.0.0"
    },
    "volta": {
        "node": "19.8.1"
    },
    "files": [
        "dist/*.js",
        "dist/*.d.ts",
        "dist/*.js.map",
        "dist/sync/*.js",
