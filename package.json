        "dist/sync/*.js.map"
    ],
    "dependencies": {},
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./sync": {
            "types": "./dist/sync/index.d.ts",
            "default": "./dist/sync/index.js"
        }
